{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "brace-style": ["error", "stroustrup"],
    "react/jsx-uses-vars": "error",
    "no-unused-vars": ["error", { "varsIgnorePattern": "h" }]
  }
}
